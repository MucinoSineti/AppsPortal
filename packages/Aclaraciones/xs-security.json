{
  "xsappname": "aclaraciones",
 "tenant-mode": "shared",
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "$XSAPPNAME.Callback",
      "description": "With this scope set, the callbacks for subscribe, unsubscribe and getDependencies can be called.",
      "grant-as-authority-to-apps": [
        "$XSAPPNAME(application,sap-provisioning,tenant-onboarding)"
      ]
    },
    {
      "name": "$XSAPPNAME.AclaracionesScope",
      "description": "Scope para aplicaci√≥n Aclaraciones"
    }
  ],
  "role-templates": [
    {
      "name": "AclaracionesRole",
      "description": "Rol para acceder a la app Aclaraciones",
      "scope-references": [
        "$XSAPPNAME.AclaracionesScope",
        "$XSAPPNAME(application,backend).backendscope"
      ]
    }
  ],
  "foreign-scope-references": [
    "$XSAPPNAME(application,backend).backendscope"
  ],
  "oauth2-configuration": {
    "token-validity": 14400,
    "redirect-uris": [
      "http*://*.cfapps.*.hana.ondemand.com/**"
    ]
  }
}